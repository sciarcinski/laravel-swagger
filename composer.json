{
    "name": "sciarcinski/laravel-swagger",
    "description": "OpenApi or Swagger integration to Laravel",
    "keywords": ["laravel", "swagger", "documentation", "api", "OpenApi", "API"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sciarcinski\\LaravelSwagger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sciarcinski\\LaravelSwagger\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sławomir Ciarciński",
            "email": "slawgos@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "evenement/evenement": "^3.0",
        "swagger-api/swagger-ui": "^4.15"
    },
    "require-dev": {
        "laravel/framework": "8.*|9.*|10.*",
        "laravel/pint": "^1.4",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "8.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Sciarcinski\\LaravelSwagger\\LaravelSwaggerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
